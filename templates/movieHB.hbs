{{#each movie}}
  <div id="{{imdbID}}" class="panel panel-default col-sm-4 userMovie">
    <div class="img-wrap">
      <span id="delete#{{key}}" class="close">&times;</span>
      <img src="{{Poster}}" data-id="123" alt="{{Title}}">
    </div>
    <h3>{{Title}}</h3>
    <h6>{{Actors}}</h6>
    <h6>{{Released}}</h6>
    <h5>{{Rating}}</h5>
    <button id="addMovie" type="add">Add Movie</button>
    <div>
      <span class="star-rating">
        <input type="radio" name="rating" value="1"><i></i>
        <input type="radio" name="rating" value="2"><i></i>
        <input type="radio" name="rating" value="3"><i></i>
        <input type="radio" name="rating" value="4"><i></i>
        <input type="radio" name="rating" value="5"><i></i>
      </span><span><h3 class="choice">Choose a rating</h3></span>
    </div>
  </div>
{{/each}}
