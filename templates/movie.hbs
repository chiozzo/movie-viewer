{{#each movie}}
  <div class="panel panel-default col-md-4 userMovie">
    <div class="img-wrap">
      <span id="delete#{{key}}" class="close">&times;</span>
      <img src="{{Poster}}" data-id="123" alt="Skyfall poster">
    </div>
    <h3>{{Title}}</h3>
    <h6>{{Actors}}</h6>
    <h6>{{Released}}</h6>
    <h5>Rating</h5>
  </div>
{{/each}}